<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <Button
        android:text="Start Engine"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnStart"
        android:layout_marginBottom="0.0dp" />
    <Button
        android:text="Stop Engine"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnStop" />
    <Button
        android:text="Get Clouds"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnGetClouds" />
    <Button
        android:text="Connect Cloud"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnConnectCloud" />
    <EditText
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/txtCloud"
        android:text="mw3 xbox" />
    <Button
        android:text="Leave Cloud"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnLeaveCloud" />
    <Button
        android:text="Send PM"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnSendPM" />
    <Button
        android:text="Send Chat"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnSendChat" />
    <EditText
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/txtChatMessage" />
    <Button
        android:text="Exit"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnExit" />
</LinearLayout>